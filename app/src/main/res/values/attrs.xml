<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemDashboardCardView">
        <attr name="cardTitle" format="string" />
        <attr name="value" format="string" />
        <attr name="iconTint" format="color" />
    </declare-styleable>

    <declare-styleable name="StatusChipView">
        <attr name="status">
            <flag name="red" value="0x00000001" />
            <flag name="green" value="0x00000002" />
            <flag name="blue" value="0x00000003" />
        </attr>
    </declare-styleable>
</resources>